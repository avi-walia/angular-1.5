{
  "globalstrict": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  //  "immed": true,
  //  "indent": 2,
  "latedef": false,
  // used before it was defined
  "newcap": true,
  "noarg": true,
  //"quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": false,
  // variable defined but not used
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "white": true,
  "validthis": true,
  "globals": {
    "angular": false,
    "_": false,
    "$log": false,
    "document": true,
    "confirm": false,
    "console": false,
    // Angular Mocks
    "inject": false,
    "module": false,
    // JASMINE
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "expect": false
  }
}
